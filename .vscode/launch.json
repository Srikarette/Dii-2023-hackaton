{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Java",
            "type": "java",
            "request": "launch",
            "stopOnEntry": true,
            "jdkPath": "${env:JAVA_HOME}/bin",
            "cwd": "${fileDirname}",
            "startupClass": "${fileBasenameNoExtension}",
            "classpath": [
                ".",
                "${fileDirname}"
            ]
        },
        {
            "name": "Java Console App",
            "type": "java",
            "request": "launch",
            "stopOnEntry": true,
            "jdkPath": "${env:JAVA_HOME}/bin",
            "cwd": "${fileDirname}",
            "startupClass": "${fileBasenameNoExtension}",
            "classpath": [
                ".",
                "${fileDirname}"
            ],
            "externalConsole": true
        },
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "AccountingApplication",
            "request": "launch",
            "mainClass": "user.service.accounting.AccountingApplication",
            "projectName": "accounting-service"
        },
        {
            "type": "java",
            "name": "MessageServiceApplication",
            "request": "launch",
            "mainClass": "message.service.messageservice.MessageServiceApplication",
            "projectName": "message-service"
        },
        {
            "type": "java",
            "name": "EurekaNamingServerApplication",
            "request": "launch",
            "mainClass": "spring.hk.namingserver.EurekaNamingServerApplication",
            "projectName": "naming-server"
        },
        {
            "type": "java",
            "name": "NotificationApplication",
            "request": "launch",
            "mainClass": "spring.hk.notification.NotificationApplication",
            "projectName": "notification-service"
        }
    ]
}